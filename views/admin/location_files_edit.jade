extends ../layout/default

block content
  .container
    h1 Edit location file
    form(method='post' action='/admin/location-files/#{locationFile.id}/update')
      .form-group
        label(for='location_file_url') URL for location JSON file
        input.form-control#location_file_url(type='url' placeholder='Enter url here...' name='location_file_url' value='#{locationFile.url}')
      button.btn.btn-primary(type='submit') Update
